  "dependencies": {
    "@simonwep/pickr": "^1.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "cypress": "^4.3.0",
    "koa": "^3.0.3",
    "koa-static": "^5.0.0",
    "pnpm": "^10.0.0",
    "rollup": "^2.79.2",
    "rollup-plugin-babel": "^4.4.0",
